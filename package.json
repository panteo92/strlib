{
  "name": "strlib",
  "version": "1.0.0",
  "description": "Testing Yarn Package Manager",
  "main": "index.js",
  "scripts": {
    "test": "cross-env BABEL_ENV=test ava",
    "build": "cross-env BABEL_ENV=build rollup -c"
